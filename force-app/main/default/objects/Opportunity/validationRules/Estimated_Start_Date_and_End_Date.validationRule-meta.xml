<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Estimated_Start_Date_and_End_Date</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISPICKVAL(StageName, &quot;Closed Won&quot;), ISBLANK(Estimated_Start_Date__c ), ISBLANK(Estimated_End_Date__c ))</errorConditionFormula>
    <errorMessage>Stage Closed Won must have Estimated Start Date and Estimated End Date.</errorMessage>
</ValidationRule>
